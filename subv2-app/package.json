		"stylelint-config-standard": "^25.0.0",
		"stylelint-order": "^5.0.0",
		"typescript": "^4.3.2",
		"webpack": "^5.61.0",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.4.0",
		"webpack-merge": "^5.7.3"
	}
}
